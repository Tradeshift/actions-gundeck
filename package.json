{
  "name": "@tradeshift/actions-gundeck",
  "version": "0.0.0",
  "private": true,
  "description": "Github action to trigger rundeck jobs w/ mTLS",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tradeshift/actions-gundeck.git"
  },
  "keywords": [
    "actions",
    "node",
    "rundeck"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/exec": "^1.1.1",
    "csv-parse": "^4.16.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "@vercel/ncc": "^0.33.4",
    "eslint": "^7.28.0",
    "eslint-plugin-github": "^4.1.3",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^27.4.7",
    "jest-circus": "^27.0.4",
    "prettier": "2.3.1",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.2"
  }
}
