{
  "name": "@tradeshift/actions-gundeck",
  "version": "0.0.0",
  "private": true,
  "description": "Github action to trigger rundeck jobs w/ mTLS",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tradeshift/actions-gundeck.git"
  },
  "keywords": [
    "actions",
    "node",
    "rundeck"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.11.1",
    "@actions/exec": "1.1.1",
    "csv-parse": "6.1.0"
  },
  "devDependencies": {
    "@types/jest": "29.5.14",
    "@types/node": "20.17.16",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vercel/ncc": "0.38.3",
    "eslint": "7.32.0",
    "eslint-plugin-github": "4.1.3",
    "eslint-plugin-jest": "27.9.0",
    "jest": "29.7.0",
    "jest-circus": "29.7.0",
    "prettier": "2.8.8",
    "ts-jest": "29.2.5",
    "typescript": "5.8.3"
  }
}
